{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "metal",
      "camelcase": "Metal",
      "title": "Metal",
      "scope": "source.metal",
      "file-types": [
        "metal"
      ],
      "injection-regex": "^metal$",
      "class-name": "TreeSitterMetal"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "apple's metal shading language tree sitter parser",
    "authors": [
      {
        "name": "George Potoshin",
        "email": "george.potoshin@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-metal"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}